{
  "name": "get-youtube-description",
  "description": "get-youtube-description",
  "version": "1.0.0",
  "manifest_version": 3,
  "permissions": ["storage", "activeTab", "webNavigation", "scripting", "downloads"],
  "background": {
    "service_worker": "src/background.ts",
    "type": "module"
  },
  "action": {
    "default_icon": {
      "16": "src/assets/favicon-16x16.png",
      "32": "src/assets/favicon-32x32.png"
    },
    "default_popup": "src/popup/index.html"
  },
  "icons": {
    "16": "src/assets/favicon-16x16.png",
    "32": "src/assets/favicon-32x32.png",
    "192": "src/assets/android-chrome-192x192.png"
  },
  "web_accessible_resources": [
  ],
  "host_permissions": [
    "*://*.youtube.com/*"
  ]
}